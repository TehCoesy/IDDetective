<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="252.0" layoutY="27.0" style="&#10;" text="Crop Visualizer" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <ImageView fx:id="img_input" fitHeight="300.0" fitWidth="300.0" layoutX="14.0" layoutY="68.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="img_output" fitHeight="300.0" fitWidth="300.0" layoutX="326.0" layoutY="59.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="b_previous" layoutX="14.0" layoutY="460.0" mnemonicParsing="false" text="Previous" />
            <Button fx:id="b_next" layoutX="577.0" layoutY="460.0" mnemonicParsing="false" text="Next" />
      </children>
    </AnchorPane>
  </children>
</VBox>
